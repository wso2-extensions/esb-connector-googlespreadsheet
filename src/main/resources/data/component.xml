<component name="data" type="synapse/template" displayName="Data Manipulation Operations">
    <subComponents>
        <component name="addRowsAndColumns">
            <displayName>Add Rows And Columns</displayName>
            <file>addRowsAndColumns.xml</file>
            <description>Append row/rows/column/columns of data.</description>
        </component>
        <component name="copyRange">
            <displayName>Copy Range</displayName>
            <file>copyRange.xml</file>
            <description>Copy cell data and formatting between ranges.</description>
        </component>
        <component name="cutRange">
            <displayName>Cut Range</displayName>
            <file>cutRange.xml</file>
            <description>Move data between ranges including formatting and formulas.</description>
        </component>
        <component name="sortRange">
            <displayName>Sort Range</displayName>
            <file>sortRange.xml</file>
            <description>Sort data in ranges with multiple sort specifications.</description>
        </component>
    </subComponents>
</component>
